		<resDir name="reviews" load="xml" />
		<template name="_reviews" />
		<struc name="default" />
	</fragment>

	<fragment name="booster">
		<data name="httpConfig" />
		<template name="_booster" />
		<data name="booster" />
		<struc name="default" />
	</fragment>

	<fragment name="prices">
		<template name="_prices" />

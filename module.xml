		<struc name="/core/DOM" />
		<script name="MTG" />
		<script name="MTG.Manager" />
		<script name="MTG.Deck" />
	</fragment>

	<fragment name="chat">
		<param name="chat-database" value="mtg" />
		<template name="/chat/shoutbox" />
		<call name="/chat/default" as="chat" />
	</fragment>

	<struc name="default">
		<res name="/core/sites" load="xml" />

<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform">		<xsl:import href="/getTemplate.php/mtg/global"/>	<xsl:template match="/data">		<xsl:variable name="request" select="request"/>		<html>			<head/>			<body>				<main>					<h1>Magic Booster Generator</h1>					<form action="./" method="GET">						<label>							Set:							<select name="set" required="required">								<option/>								<xsl:for-each select="data/oracle/categories/set">									<option>										<xsl:if test=". = $request/param[@name='set']">											<xsl:attribute name="selected">selected</xsl:attribute>										</xsl:if>										<xsl:value-of select="."/>									</option>								</xsl:for-each>							</select>						</label>						<button type="submit">Generate!</button>					</form>					<xsl:call-template name="deck">						<xsl:with-param name="deck" select="data/oracle/search"/>						<xsl:with-param name="mode" select="'booster'"/>					</xsl:call-template>				</main>			</body>		</html>	</xsl:template></xsl:stylesheet>
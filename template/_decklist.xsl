<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform">		<!--	<xsl:variable name="uri" select="'http://magiccards.info/extras/token/return-to-ravnica/knight.jpg'"/>	<xsl:variable name="uri" select="'http://magiccards.info/extras/token/avacyn-restored/human-1.jpg'"/>	-->	<xsl:template match="/data/data[@data-cms-name = 'show']">		<html>			<head>				<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no"/><style type="text/css"><![CDATA[		* {	padding: 0;	margin: 0;	font-size: 1em;}img {	display: block;}body {	vertical-align: middle;	height: 100%;	font-family: sans-serif;	font-size: 12px;}div.wrapper {	width: 18cm;	height: 26cm;	margin: 0 auto;	padding: 0.5cm 0;	page-break-after: always;}header {	height: 4cm;}main {	height: 22cm;}header table {	border-collapse: collapse;	margin: auto;	width: 100%;}td, th {	text-align: left;}td.number {	text-align: right;	width: 5em;}td.big {	font-size: 1.3em;}td.wide {	width: 20em;}td.border {	border: 2px solid black;	height: 2em;	line-height: 2em;}td.label {	padding: 0 0.5em;}td.right {	text-align: right;}td.dark {	width: 2em;}.dark {	background-color: rgba(0, 0, 0, 0.1);}.cardListTable {	display: table;	width: 100%;	height: 100%;}.cardListTable > * {	display: table-cell;	vertical-align: top;	width: 45%;	position: relative;}.cardListTable > *:first-child {	width: 10%;}.cardList {	max-height: 22cm;	min-height: 11cm;	overflow: auto;}.cardList table {	width: 100%;	border-spacing: 1em 0.5em;}.cardList td {	border-bottom: 1px solid gray;	padding: 0 0.25em;}.cardList thead {	font-size: 1.1em;}.input, input {	font-family: monospace;	font-weight: bold;}input {	border: none;	padding: 0 0.5em;	display: block;	width: 100%;	line-height: inherit;	background: transparent;	box-sizing: border-box;}input.dci {	text-align: center;}input:hover {	background-color: rgba(0, 255, 0, 0.1);}.rotate {	width: 2cm;	position: relative;}.rotate > div {	transform: rotate(-90deg);	position: absolute;	bottom: 0;	left: 4em;	box-sizing: border-box;	transform-origin: 0 100%;	width: 25cm;	height: 4em;}.rotate table {	border-collapse: collapse;	border: 2px solid black;}.rotate table td {	height: 4em;	line-height: 4em;}.rotate table td.label {	padding: 0 1em;}.rotate table td + td {	border-left: 1px solid silver;}.rotate table td.wide {	width: 15em;}.rotate input {	font-size: 1.5em;}.rotate input.dci {	letter-spacing: 5px;}.cardCount {	display: table;	width: 100%;	border-spacing: 1em 0;}.cardCount > * {	display: table-cell;	line-height: 2em;}.cardCount > *:first-child {	font-weight: bold;}.cardCount > *:last-child {	border: 2px solid black;	width: 4em;	text-align: center;}.bottom {	position: absolute;	bottom: 0;	width: 100%;}.tinyTable {	margin-top: 0.5em;	margin-right: 1em;	width: 100%;	border-collapse: collapse;	border: 2px solid black;	table-layout: fixed;}.tinyTable tbody {	font-size: 0.6em;}.tinyTable td {	border: 1px solid black;	padding:  0 0.5em;}			]]></style>			</head>			<body>				<xsl:for-each select="deck">					<div class="wrapper">						<header>							<table>								<tr>									<td class="big" colspan="2">DECK REGISTRATION SHEET</td>									<td/>									<td class="right label">First Letter of Last Name</td>									<td class="border dark"><input value="{substring(@player-lastname, 1, 1)}" class="dci"/></td>								</tr>								<tr>									<td class="border right label">Date:</td>									<td class="border wide"><input value="{@date}"/></td>									<td class="border right label">Event:</td>									<td class="border wide" colspan="2"><input value="{@event}"/></td>								</tr>								<tr>									<td class="border right label">Location:</td>									<td class="border wide"><input value="{@location}"/></td>									<td class="border right label">Deck Name:</td>									<td class="border wide" colspan="2"><input value="{@name}"/></td>								</tr>								<tr>									<td colspan="2"><!--PRINT CLEARLY USING ENGLISH CARD NAMES--></td>									<td class="border right label">Deck Designer:</td>									<td class="border wide" colspan="2"><input value="{@player}"/></td>								</tr>							</table>						</header>						<main>							<div class="cardListTable">								<div class="rotate">									<div>										<table class="dark">											<tr>												<td class="label">Last Name:</td>												<td class="wide"><input value="{@player-lastname}"/></td>												<td class="label">First Name:</td>												<td class="wide"><input value="{@player-firstname}"/></td>												<td class="label">DCI #:</td>												<td class="wide"><input value="{@player-dci}" class="dci"/></td>											</tr>										</table>									</div>								</div>								<div>									<xsl:call-template name="cardList">										<xsl:with-param name="cardList" select="card[not(contains(@type, 'Land'))]"/>										<xsl:with-param name="title" select="'Main Deck:'"/>									</xsl:call-template>									<div class="bottom">										<div class="cardCount">											<span>Total Number of Cards in Main Deck:</span>											<span class="input"><xsl:value-of select="sum(card/@stock)"/></span>										</div>									</div>								</div>								<div>									<xsl:call-template name="cardList">										<xsl:with-param name="cardList" select="card[contains(@type, 'Land')]"/>										<xsl:with-param name="title" select="'Main Deck Continued &amp; Basic Lands:'"/>									</xsl:call-template>									<xsl:call-template name="cardList">										<xsl:with-param name="cardList" select="sideboard/card"/>										<xsl:with-param name="title" select="'Sideboard:'"/>									</xsl:call-template>									<div class="bottom">										<div class="cardCount">											<span>Total Number of Cards in Sideboard:</span>											<span class="input"><xsl:value-of select="sum(sideboard/card/@stock)"/></span>										</div>										<table class="tinyTable">											<tbody>												<tr><td>FOR OFFICIAL USE ONLY</td><td>Main/SB:</td></tr>												<tr><td>Deck Check Rd #:</td><td>Deck Check Rd #:</td></tr>												<tr><td>Status:</td><td>Status:</td></tr>												<tr><td>Judge:</td><td>Judge:</td></tr>											</tbody>										</table>									</div>								</div>							</div>						</main>					</div>				</xsl:for-each>			</body>		</html>	</xsl:template>		<xsl:template name="cardList">		<xsl:param name="cardList" select="/.."/>		<xsl:param name="title" select="''"/>		<div class="cardList">			<table>				<thead>					<tr>						<th colspan="2"><xsl:value-of select="$title"/></th>					</tr>					<tr>						<td># in deck:</td>						<td>Card Name:</td>					</tr>				</thead>				<tbody class="input">					<xsl:for-each select="$cardList">						<tr>							<td class="number"><xsl:value-of select="@stock"/></td>							<td><xsl:value-of select="@name"/></td>						</tr>					</xsl:for-each>				</tbody>			</table>		</div>	</xsl:template></xsl:stylesheet>
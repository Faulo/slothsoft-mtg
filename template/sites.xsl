<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:template match="player">		<page name="{@key}" title="{@name}" ref="player" module="mtg" status-active="" status-public="">			<!--			<xsl:for-each select="deck[@type][not(@type = 'managed')]">				<page name="{@no}" title="{@name}" redirect="view" module="mtg" status-active="" status-public="">					<xsl:for-each select="mode">						<page name="{@name}" title="{@name}" ref="deck" module="mtg" status-active="" status-public="">						</page>					</xsl:for-each>				</page>			</xsl:for-each>			-->			<page name="Manager" title="Manager" ref="manager" status-active="" status-public=""/>			<xsl:for-each select="deck">				<page name="{@no}" title="{@name}" redirect="view" status-active="" status-public="">					<xsl:for-each select="mode">						<page name="{@name}" title="{@name}" ref="deck" status-active="" status-public="">						</page>					</xsl:for-each>					<page ext="/getFragment.php/mtg/show?deck={@key}" title="Permalink" status-active="" status-public=""/>					<page ext="/getFragment.php/mtg/decklist?deck={@key}" title="Decklist" status-active="" status-public=""/>					<page ext="/getFragment.php/mtg/print?deck={@key}" title="Print" status-active="" status-public=""/>					<page name="move-up" title="[▲] Move" ref="deck-edit" status-active="" status-public=""/>					<page name="move-down" title="[▼] Move" ref="deck-edit" status-active="" status-public=""/>					<page name="delete" title="[x] Delete" ref="deck-edit" status-active="" status-public=""/>				</page>			</xsl:for-each>			<page name="{count(deck[number(@no) = @no]) + 1}" title="[+] New Deck" redirect="new" status-active="" status-public="">				<page name="new" title="[+] New Deck" ref="deck-edit" status-active="" status-public=""/>			</page>		</page>	</xsl:template></xsl:stylesheet>